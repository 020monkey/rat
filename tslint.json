{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-plugin-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": [{
      "singleQuote": true,
    }],
    "no-console": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "array-type": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"]
  },
  "rulesDirectory": []
}
