{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true
      }
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
