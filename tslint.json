{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-reference": false,
        "no-namespace": false,
        "trailing-comma": false,
        "prefer-const": false,
        "object-literal-sort-keys": false,
        "align": false,
        "max-classes-per-file": false,
        "prefer-for-of": false,
        "no-console": false,
        "array-type": false,
        "interface-name": false
    },
    "rulesDirectory": []
}